tipo Conta
  nome: Texto
  numero: Texto
  var _saldo := 0.0

  deposito(valor: Real): L처gico
    se valor >= 0 ent찾o _saldo := _saldo + valor fim
    retorne valor >= 0
  fim

  saque(valor: Real): L처gico
    se valor >= 0 ent찾o _saldo := _saldo - valor fim
    retorne valor >= 0
  fim

  saldo() = _saldo
fim

conta = Conta("James", "007")
escreva conta.saldo
conta.deposito(100.0)
